           <div style={{ width: '100%', height: '2px', backgroundColor: '#E49C00' }} className=\"mb-4\" ></div>\n                {this.state.hives.map((hive) => {\n                  return (\n                    <div className={(this.state.selectedHive === hive.id) ? 'ruche ruche-me' : 'ruche'} data-hive={hive.id} onMouseEnter={this.displayImg.bind(this)}>\n                      <img src={require(\"../../assets/img/rayon.png\")} className=\"img-fluid rayon\" alt=\"Rayon\" />\n                      <p style={{ padding: '0px', margin: '0', overflowX: 'visible' }} className=\"\" >{hive.name.toUpperCase()}</p>\n                      <Link to={'/hive/' + hive.id} style={{ color: '#666666', fontSize: '0.9em', lineHeight: '1em' }} >Voir en détails</Link>\n                    </div>\n                  )\n                })}\n                <div style={{ position: 'relative' }}>\n                  {this.state.hives.map((hive) => {\n                    return (\n                      <div style={{ backgroundImage: 'url(' + process.env.REACT_APP_CONTENT_DOMAIN + '/' + hive.imgs[0] + ')', height: '10em', width: '100%' }} alt={hive.name} className={(this.state.selectedHive === hive.id) ? 'ruche-img' : 'ruche-img-hidden'} id={\"img-\" + hive.id} />\n                    )\n                  })}\n                </div>\n              </div>\n            </div>\n            <div className=\"row justify-content-end align-items-center mb-4\">\n              <div className=\"col-lg-12\">\n                <h2 style={{ color: '#E49C00' }}>{(this.state.english) ? 'LATEST NEWS' : 'LES DERNIÈRES ACTUALITÉS'}</h2>\n                <div style={{ width: '100%', height: '1px', backgroundColor: '#E49C00' }} className=\"mb-4\" ></div>\n                {this.state.user && this.state.news.map((actu) => {\n                  return (\n                    <div className=\"my-2 row\">\n                      <div className=\"actu-first-block col-lg-4\">\n                        <Imagebox className=\"actu-img\"\n                          src={process.env.REACT_APP_CONTENT_DOMAIN + '/' + actu.img}\n                          alt={actu.img}\n                        />\n                      </div>\n                      <div className=\"actu-second-block col-lg-8\">\n                        <h3 className=\"actu-title\">{actu.title}<small className=\"actu-date\">{moment(actu.date).format(\"DD.MM.YYYY\")}</small></h3>\n                        <div style={{ width: '100%', height: '1px', backgroundColor: '#E49C00' }} className=\"mb-4\" ></div>\n                        <div className=\"actu-content\">\n                          <p>\n                            {ReactHtmlParser(actu.content)}\n                          </p>\n                        </div>\n                        <p className=\"actu-btn\" data-deployed=\"false\" onClick={this.deployActu.bind(this)}>------- développer --------------------------------------------------------------------------------------------------------------------------------------------------------------------------</p>\n                      </div>\n                    </div>\n                  )\n                })}\n              </div>\n            </div>\n            <FooterPage english={this.state.english} />\n          </div></div> :\n          <div className=\"container my-5\">\n            <Loading />\n            <p className=\"text-center\">Chargement de la page entreprise...</p>\n          </div>}\n      </div>\n    );\n  }\n});\n","/Users/cchampouillon/code/confidences-app/src/components/company/Presentation.jsx",[],"/Users/cchampouillon/code/confidences-app/src/components/company/Prices.jsx",["1883"],"import React from 'react';\nimport ButtonLink from '@cda/button-link';\nimport { Item, Rows } from '@cda/flex';\nimport Meta from '../utils/Meta';\nimport PriceCard from '../PriceCard';\n\nconst UNIT = '/ an / ruche';\n\nconst tenOffer = [\n  { id: 'beehive', value: 'Ruches aux couleurs de votre entreprise' },\n  { id: 'page', value: 'Une page dédiée à votre entreprise' },\n  { id: 'miel', value: '40 pots de miel de 250g par ruche', bold: true },\n  { id: 'post', value: 'Frais d\\'expedition inclus' },\n];\n\nconst twentyOffer = [\n  { id: 'beehive', value: 'Ruches aux couleurs de votre entreprise' },\n  { id: 'page', value: 'Une page dédiée à votre entreprise' },\n  { id: 'miel', value: '40 pots de miel de 250g par ruche', bold: true },\n  { id: 'post', value: 'Frais d\\'expedition inclus' },\n];\n\nexport default () => (\n  <>\n    <Meta title=\"Tarifs particuliers\" />\n    <ButtonLink url=\"/individual/prices\" primary>Tarifs particuliers ></ButtonLink>\n    <h2 className=\"text-center my-4\">Tarifs entreprises</h2>\n    <Rows>\n      <PriceCard title=\"1 à 4 ruches\" price={635} items={tenOffer} unit={UNIT} level={1} />\n      <PriceCard title=\"5 ruches ou +\" price={560} items={twentyOffer} unit={UNIT} level={2} />\n    </Rows>\n    <Rows>\n      <Item textAlign=\"center\">\n        <ButtonLink url=\"/signup/company\" primary>Parrainer maintenant</ButtonLink>\n      </Item>\n    </Rows>\n  </>\n);\n","/Users/cchampouillon/code/confidences-app/src/components/company/manage/Account.jsx",["1884"],"import React, { Component } from 'react';\nimport { Redirect } from 'react-router-dom';\nimport FontAwesome from 'react-fontawesome';\n\nimport request from '../../../services/Net';\nimport Confirm from '../../utils/Confirm';\nimport Loading from '../../utils/Loading';\nimport { logout } from '../../../services/AuthService';\nimport { handleChange } from '../../../services/FormService';\nimport Meta from '../../utils/Meta';\nimport { withNotification } from '../../../services/withNotification';\n\nexport default withNotification(class Account extends Component {\n  state = {\n    logout: false,\n    newsletter: null,\n    email: '',\n    firstname: '',\n  };\n\n  componentDidMount() {\n    const { notification } = this.props;\n    request({\n      url: '/user/me',\n      method: 'get',\n    }, notification).then((res) => {\n      this.setState({\n        email: res.email,\n        firstname: res.firstname,\n      });\n      request({\n        url: `/newsletter/${res.email}/17334`,\n        method: 'get',\n      }, notification).then((res) => {\n        this.setState({ newsletter: res });\n      });\n    });\n  }\n\n  deleteAccount() {\n    const { notification } = this.props;\n    request({\n      url: '/user',\n      method: 'delete',\n    }, notification).then((res) => {\n      logout();\n      this.setState({\n        logout: true,\n      });\n    });\n  }\n\n  changePassword(e) {\n    e.preventDefault();\n    const { notification } = this.props;\n    if (this.state.password === this.state.conf) {\n      request({\n        url: '/user',\n        method: 'put',\n        data: {\n          password: this.state.password,\n        },\n      }, notification);\n    } else {\n      notification.addNotification({\n        message: 'Le nouveau mot de passe et sa confirmation ne correspondent pas',\n        level: 'warning',\n      });\n    }\n  }\n\n\n  updateNewsletter(e) {\n    const { notification } = this.props;\n    const stat = e.target.value;\n    request({\n      url: '/user/newsletter',\n      method: 'PUT',\n      data: {\n        status: stat,\n      },\n    }, notification).then((res) => {\n      this.setState({\n        newsletter: !stat.localeCompare('true'),\n      });\n    });\n  }\n\n  sub(e) {\n    const { notification } = this.props;\n    request({\n      url: '/newsletter',\n      method: 'post',\n      data: {\n        email: this.state.email,\n        listId: '17334',\n        firstname: this.state.firstname,\n      },\n    }, notification).then((res) => {\n      this.setState({\n        newsletter: !this.state.newsletter,\n      });\n    });\n  }\n\n  unsub() {\n    const { notification } = this.props;\n    request({\n      url: `/newsletter/${this.state.email}/17334`,\n      method: 'delete',\n    }, notification).then((res) => {\n      this.setState({\n        newsletter: !this.state.newsletter,\n      });\n    });\n  }\n\n  render() {\n    return (\n      <div className=\"row\">\n        <Meta title=\"Mon compte\" />\n        {this.state.logout && <Redirect to=\"/\" />}\n        <div className=\"col-lg-12\">\n          <h2 className=\"text-center my-5\">Mon compte</h2>\n          <div className=\"row\">\n            <div className=\"col-lg-6 text-center\">\n              <h3 className=\"text-center my-4\"><small>Modifier mon mot de passe</small></h3>\n              <form onSubmit={this.changePassword.bind(this)}>\n                <div className=\"form-group\">\n                  <input type=\"password\" name=\"password\" onChange={handleChange.bind(this)} value={this.state.password} className=\"form-control\" placeholder=\"Nouveau mot de passe\" />\n                </div>\n                <div className=\"form-group\">\n                  <input type=\"password\" name=\"conf\" onChange={handleChange.bind(this)} value={this.state.conf} className=\"form-control\" placeholder=\"Confirmation du nouveau mot de passe\" />\n                </div>\n                <button className=\"btn btn-primary mb-4\">Enregistrer</button>\n              </form>\n              <h3 className=\"my-2\"><small>Newsletter</small></h3>\n              <hr />\n              {(this.state.newsletter === null)\n                ? <Loading />\n                : (this.state.newsletter)\n                  ? (\n                    <div>\n                      <p>Vous êtes inscrit à la newsletter.</p>\n                      <div className=\"text-center\">\n                        <button className=\"btn btn-secondary btn-sm\" value={false} onClick={this.unsub.bind(this)}>Me désinscrire</button>\n                      </div>\n                    </div>\n                  )\n                  : (\n                    <div>\n                      <p>Vous n'êtes pas inscrit à la newsletter.</p>\n                      <div className=\"text-center\">\n                        <button className=\"btn btn-secondary btn-sm\" value onClick={this.sub.bind(this)}>M'inscrire</button>\n                      </div>\n                    </div>\n                  )\n                        }\n            </div>\n            <div className=\"col-lg-6 text-center\">\n              <h3 className=\"text-center my-4\"><small>Supprimer mon compte</small></h3>\n              <p className=\"alert alert-danger\">\n                <FontAwesome name=\"warning\" />\n                {' '}\nATTENTION ! Vous allez supprimer votre compte ainsi toutes les informations vous concernant. Il ne sera pas possible de revenir en arrière. En confirmant la suppression de votre compte vous renoncez en même temps à vos droits liés à votre compte ainsi qu’à ceux liés à la souscription d’un parrainage ; vous résiliez votre parrainage quand bien même celui-ci ne serait pas arrivé à son terme. Cette action est permanente et nous ne serons pas capables de retrouver vos données.\n              </p>\n              <Confirm action={this.deleteAccount.bind(this)} text=\"Supprimer mon compte\" class=\"btn btn-sm btn-danger\" />\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n});\n","/Users/cchampouillon/code/confidences-app/src/components/company/manage/Bills.jsx",["1885"],"import React, { Component } from 'react';\nimport moment from 'moment';\nimport FontAwesome from 'react-fontawesome';\n\nimport request from '../../../services/Net';\nimport { withNotification } from '../../../services/withNotification';\n\nexport default withNotification(class CompanyManageBills extends Component {\n  state = {\n    user : null,\n    bills: [],\n  };\n\n  componentDidMount() {\n    const { notification } = this.props;\n    request({\n      url: '/bill/mine',\n      method: 'get',\n    }, notification).then((res) => {\n      this.setState({\n        bills: res,\n      });\n    });\n  }\n\n  render () {\n    return (\n      <di