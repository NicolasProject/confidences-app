ding: true\n    });\n    request({\n      url: '/label/generate',\n      method: 'post',\n      data: {\n        model: this.state.model,\n        mention: this.state.mention\n      }\n    }, notification).then((res) => {\n      this.get();\n    });\n  }\n\n  render () {\n    return (\n    <div>\n      <Meta title=\"Mes étiquettes personnalisées\"/>\n      <h2 className=\"text-center my-2\">Personnaliser mes pots de miel</h2>\n      <div className=\"row\">\n        <div className=\"col-lg-12 text-center\">\n          {this.state.step === 0 &&\n            ((this.state.label === '')?\n              <Loading />\n              :\n              <div>\n                <p>Visuel actuel de votre étiquette. Pour le modifier veuillez cliquer sur \"Recommencer la personnalisation\" en dessous de celui-ci.</p>\n                <Document file={process.env.REACT_APP_CONTENT_DOMAIN+'/label/'+this.state.label} >\n                  <Page pageNumber={1} width={500} className=\"label\" />\n                </Document>\n                <button className=\"btn btn-secondary\" onClick={() => { this.setState({ step : 1 }); }}>Recommencer la personnalisation <FontAwesome name=\"magic\" /></button>\n              </div>)\n          }\n          {this.state.step  === 1 &&\n            <div className=\"row\">\n              <div className=\"col-lg-12\">\n                <p>Choisissez un modèle en cliquant sur l'un d'eux. Vous serez amené à saisir vos informations à l’étape suivante.</p>\n              </div>\n              <div className=\"col-lg-6\">\n                <img className={(this.state.model === 1)?'img-fluid model-etiq':\"img-fluid\"} src={Eti1} onClick={() => { this.setState({ model : 1 }); }} alt=\"Modèle 1\"/>\n              </div>\n              <div className=\"col-lg-6\">\n                <img className={(this.state.model === 2)?'img-fluid model-etiq':\"img-fluid\"} src={Eti2} onClick={() => { this.setState({ model : 2 }); }} alt=\"Modèle 2\"/>\n              </div>\n              <div className=\"col-lg-12 text-center mt-4\">\n                <button className=\"btn btn-primary\" onClick={() => { this.setState({ step: 2 })}}>Étape suivante <FontAwesome name=\"chevron-right\" /></button>\n              </div>\n            </div>\n          }\n          {this.state.step === 2 &&\n            <div className=\"row justify-content-center\">\n              <div className=\"col-lg-12\">\n                <p>Veuillez saisir le nom ou la mention qui doit figurer sur votre étiquette. Par défaut il s'agit de votre prénom nom. Vous êtes limité à 38 caractères.</p>\n              </div>\n              <div className=\"col-lg-6\">\n                <input type=\"text\" className=\"form-control\" name=\"mention\" placeholder=\"Mention au centre de l'étiquette...\" value={this.state.mention} onChange={handleChange.bind(this)} />\n              </div>\n              <div className=\"col-lg-12 text-center mt-4\">\n                <button className=\"btn btn-primary\" onClick={() => { this.setState({ step : 3 }); this.generate(); } }>Étape suivante <FontAwesome name=\"chevron-right\" /></button>\n              </div>\n            </div>\n          }\n          {this.state.step === 3 &&\n            <div className=\"row\">\n              <div className=\"col-lg-12\">\n                {(this.state.loading)?\n                  <div>\n                    <Loading />\n                    <p>Génération de votre étiquette personnalisée en cours...</p>\n                  </div>\n                :\n                <div>\n                  <p className=\"lead\">Voici le rendu de votre étiquette personnalisée !</p>\n                  <p>Les mentions entre parenthèses sont provisoires et seront remplacées lorsque les informations seront disponibles.</p>\n                  <Document file={process.env.REACT_APP_CONTENT_DOMAIN+'/label/'+this.state.label} >\n                    <Page pageNumber={1} width={500} className=\"label\" />\n                  </Document>\n                  <button className=\"btn btn-secondary\" onClick={() => { this.setState({ step : 1 }); }}>Recommencer la personnalisation <FontAwesome name=\"magic\" /></button>\n                </div>}\n              </div>\n            </div>\n          }\n        </div>\n      </div>\n    </div>\n    )\n  }\n});\n","/Users/cchampouillon/code/confidences-app/src/components/ruches/List.jsx",["3132","3133","3134","3135","3136","3137","3138","3139","3140"],"import React, { Component } from 'react';\n\nexport default class RuchesList extends Component {\n\n    render () {\n        return (\n            <div>\n                <h1>Les ruches parrainees</h1>\n            </div>\n        );\n    }\n}\n","/Users/cchampouillon/code/confidences-app/src/components/static/About.jsx",["3141","3142","3143","3144"],"import React from 'react';\n\nimport { withTranslation } from 'react-i18next';\nimport Meta from '../utils/Meta';\nimport CoverPic from '../../assets/img/about.jpg';\nimport Cover from '../Cover';\n\nexport default withTranslation('about')(({ t }) => (\n  <>\n    <Meta title=\"Histoire\" />\n    <Cover img={CoverPic}>\n      <div className=\"row mt-4 justify-content-center align-items-center\">\n        <div className=\"col\">\n          <h2>{t('quote')}</h2>\n        </div>\n        <div className=\"col\">\n          <h3>{t('title1')}</h3>\n          <p>{t('block1')}</p>\n        </div>\n      </div>\n      <h3>Une évidence</h3>\n      <p>\n        Pas de révélations ni de plans sur la comète !\n        Tout simplement le moyen pour deux apiculteurs de vivre à fond leur passion et surtout, de la faire connaître.\n        Confidences d’Abeilles s’est donc imposée à nous comme une suite logique, un moyen de continuer de rêver les yeux ouverts.\n      </p>\n      <h3>Aujourd'hui</h3>\n      <p>\n        Confidences d’Abeilles, portée par le travail de plusieurs passionnés, poursuit son vol vers de nouveaux horizons.\n        Outre le travail d’apiculteur ou plutôt celui de gardien de la\n        biodiversité, c’est le partage et la découverte que nous mettons en avant chez Confidences\n            d’Abeilles.\n        {' '}\n        <br />\n        Nous sommes convaincus que prendre des engagements forts tels que la maîtrise totale de\n        la fabrication ou le travail avec des artisans passionnés est la plus belle manière de vous\n            proposer des produits d’exception.\n        {' '}\n        <br />\n        Ce modèle philanthropique est certes louable, mais sans soutien il ne peut pas perdurer.\n        {' '}\n        <br />\n        <br />\n        C’est tout l’objet du parrainage de ruches mis en place par Confidences d’Abeilles et\n            accessible sur\n        {' '}\n        <a href=\"https://parrainagederuches.fr\" rel=\"noopener noreferrer\" target=\"_blank\"> parrainagederuches.fr</a>\n. Via ce service, nous vous invitons à ouvrir les yeux\n    sur le monde des abeilles, à vous émerveiller devant leur génie et à les soutenir.\n    Cette action est aussi le moyen pour nous de pérenniser notre activité dans un secteur en\n    proie à diverses difficultés.\n      </p>\n      <h3>Demain ... </h3>\n      <p>\n        Confidences d’Abeilles soutient\n        {' '}\n        <a href=\"http://connecthive.com/\" rel=\"noopener noreferrer\" target=\"_blank\">ConnectHive</a>\n        {' '}\ndans le développement de l’apiculture connectée de demain.\n          Elle apporte également son aide à l’association\n        {' '}\n        <a href=\"https://emapiblog.wordpress.com/\" rel=\"noopener noreferrer\" target=\"_blank\">EMAPI</a>\n, association d’apiculture des Mines d’Alès, dans la gestion de son rucher.\n      </p>\n      <p className=\"text-right my-4\">Nicolas &amp; Gaëtan &amp; les abeilles…</p>\n    </Cover>\n  </>\n));\n","/Users/cchampouillon/code/confidences-app/src/components/static/Apply.jsx",["3145"],"import React, { Component } from 'react';\n\nimport { handleChange } from '../../services/FormService';\nimport 